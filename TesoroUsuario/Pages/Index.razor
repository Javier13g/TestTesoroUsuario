@page "/"

<AuthorizeView>
    <Authorized>
        <h3 href="Identity/Account/Manage">Hello, @context.User.Identity.Name!, sus tesoros estan a salvo con nosotros</h3>
        <NavLink class="nav-link" href="indexTesoro">
        <button class="mdc-button mdc-button--raised">
            <span class="mdc-button__label">Tesoros</span>
        </button>
        </NavLink>
    </Authorized>
    <NotAuthorized>
        No has iniciado sesion, no puede visualizar sus tesoros
    </NotAuthorized>
</AuthorizeView>
